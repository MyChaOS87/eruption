name = "Heatmap"
description = "Display a heat map of previously recorded statistics"
version = "0.0.2"
author = "The Eruption Development Team"
min_supported_version = "0.1.11"
tags = ['Effect', 'Vendor']

[[config]]
type = 'color'
name = 'color_cold'
description = 'Gradient start'
default = 0xff00ff00

[[config]]
type = 'color'
name = 'color_hot'
description = 'Gradient stop'
default = 0xffff0000

[[config]]
type = 'string'
name = 'histogram_name'
description = 'Name of the histogram in the persistent data store'
default = 'key_histogram'
